{
  "name": "hr-click-ekg-monitor",
  "version": "1.1.0",
  "main": "main.js",
  "description": "A companion utility for the Microchip IdeaBuilder Workshop - Prototyping with Sensors",
  "author": {
    "name": "Garrett Scott",
    "email": "Garrett.A.Scott@gmail.com"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/idea-builder-demos/hr-click-ekg"
  },
  "license": "MIT",
  "dependencies": {
    "highcharts": "^5.0.14",
    "jquery": "^3.2.1",
    "serialport": "^6.0.0-beta1"
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.20.1",
    "electron-packager": "^8.7.2",
    "electron-winstaller": "^2.6.3"
  },
  "build": {
    "appId": "hr-click-ekg-monitor",
    "artifactName": "${productName}.${ext}",
    "compression": "maximum",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}
